wB,QAA9D,EAAwE;AACtEH,IAAAA,MAAM,CAACI,OAAP,CAAe,YAAM;AAAA,UACXC,GADW,GACHL,MADG,CACXK,GADW,EAGnB;;AACAA,MAAAA,GAAG,CAACC,KAAJ,CAAU,cAAV,EAA0B;AAAA,eAAM9B,WAAW,CAAC,GAAD,CAAjB;AAAA,OAA1B;AAEAO,MAAAA,QAAQ,CAACwB,gBAAT,CAA0B,OAA1B,EAAmC,UAAAC,CAAC,EAAI;AACtC;AACA,YAAIA,CAAC,CAACC,MAAF,CAASC,OAAT,CAAiB,iDAAjB,CAAJ,EAAyE;AACvElC,UAAAA,WAAW;AACZ,SAJqC,CAMtC;;;AACA,YAAIgC,CAAC,CAACC,MAAF,CAASC,OAAT,CAAiB,UAAjB,CAAJ,EAAkC;AAChCjB,UAAAA,SAAS,CAACe,CAAC,CAACC,MAAH,CAAT;AACD;AACF,OAVD;AAWD,KAjBD;AAkBD;AACF,CArBD","sourcesContent":["// https://v1.vuepress.vuejs.org/guide/basic-config.html#app-level-enhancements\nconst openDetails = (timeout = 150) => {\n  setTimeout(() => { // wait for hash to change after link click\n    const { hash } = location\n    if (hash && hash.length) {\n      setTimeout(() => {\n        const el = document.getElementById(hash.substr(1))\n        if (el && el.tagName.toLowerCase() === 'details') {\n          el.setAttribute('open', true)\n          const { offsetTop } = el\n          window.scrollTo({ top: offsetTop })\n        }\n      }, timeout)\n    }\n  }, 150)\n}\n\nconst openVideo = embedEl => {\n  const lazyAttr = 'data-src'\n  const iframe = embedEl.querySelector(`iframe[${lazyAttr}]`)\n  if (iframe) {\n    const src = iframe.getAttribute(lazyAttr)\n    iframe.setAttribute('src', src)\n  }\n}\n\nexport default ({ router }) => {\n  if (typeof process === 'undefined' || process.env.VUE_ENV !== 'server') {\n    router.onReady(() => {\n      const { app } = router\n\n      // initial page rendering\n      app.$once('hook:mounted', () => openDetails(500))\n\n      document.addEventListener('click', e => {\n        // faq details\n        if (e.target.matches('.sidebar-link,.header-anchor,[class*=\"algolia\"]')) {\n          openDetails()\n        }\n\n        // youtube previews\n        if (e.target.matches('.ytEmbed')) {\n          openVideo(e.target)\n        }\n      })\n    })\n  }\n}\n"]}]}